package com.globallogic.July20;
import java.util.*;

public class MyLetter {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str ="umbrella";
		for(int i=0;i<=str.length();i++){
			if(str.charAt(i)=='e'){
				System.out.println("Got letter");
				
			}
		}

	}

}
