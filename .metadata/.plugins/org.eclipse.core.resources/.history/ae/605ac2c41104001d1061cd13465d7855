package com.globallogic.Day3;

class Ram {
	int id;
	static String name="hanuman";

	Ram(int id) {
		this.id = id;

	}

	 void display(){
		System.out.println(id+" "+name);
	}
}

public class MyStatic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Ram obj= new Ram(11);
		Ram obj1=new Ram(15);
		obj.display();

	}

}
