package com.globallogic.helloworld;

public class Setter {
	private int id;
	private String name;
	private long salary;
	Setter(int id, String name, long salary){
		this.id=id;
		this.name=name;
		this.salary=salary;
	}
	public void b(){
		System.out.println(id);
		System.out.println(name);
		System.out.println(salary);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Setter ob= new Setter(25,"Shree Ram",2555);
		ob.b();
		

	}

}
