package com.globallogic.helloworld;

public class ColorCode {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch='a';
		switch(ch){
		case 'r':
			System.out.println("color is red");
			break;
			default:
				System.out.println("No color found");
				
				
			
		}
			
		

	}

}
