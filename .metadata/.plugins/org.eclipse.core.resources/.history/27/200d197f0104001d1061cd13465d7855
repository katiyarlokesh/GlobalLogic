package com.globallogic.Day3;

class A1 {

	 A1() {
		System.out.println("constructor of base class");

	}

}
class B1 extends A1{
	B1(){
		super();
		System.out.println("child class");
	}
	
}

public class SuperKeyword {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		B1 obj =new B1();

	}

}
